{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "space-before-function-paren": [
            2,
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "no-unused-vars": 0
    },
    "plugins": [
        "html"
    ],
    "globals": {
        "CustomElements": false,
        "HTMLImports": false,
        "Polymer": false,
        "WeakMap": false
    }
}